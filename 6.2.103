      {Pk_Tab,	    TAB, NUL},
      {Pk_Escape,	    ESC, NUL},
      {NL,	    NL,	 NUL},
!     {CAR,	    CAR,  NUL},
  
      /* End of list marker: */
      {0,		0, 0}
